{
    "name": "moab-tech/procore-php-sdk",
    "description": "Procore PHP SDK",
    "keywords": [
        "moab-tech",
        "procore-php-sdk"
    ],
    "homepage": "https://github.com/moab-tech/procore-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Baswell",
            "email": "chris@moab-tech.com",
            "homepage": "https://moab-tech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "MoabTech\\Procore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoabTech\\Procore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
