{
    "name": "moab-tech/procore-php-sdk",
    "description": "Procore PHP SDK",
    "keywords": [
        "moab-tech",
        "procore-php-sdk"
    ],
    "homepage": "https://github.com/moab-tech/procore-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Baswell",
            "email": "chris@moab-tech.com",
            "homepage": "https://moab-tech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-xml": "*",
        "php-http/cache-plugin": "^1.7",
        "php-http/client-common": "^2.3",
        "php-http/discovery": "^1.12",
        "php-http/httplug": "^2.2",
        "php-http/multipart-stream-builder": "^1.1",
        "psr/cache": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/deprecation-contracts": "^2.1",
        "symfony/options-resolver": "^3.4 || ^4.0 || ^5.0",
        "symfony/polyfill-php80": "^1.17"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11",
        "bamarni/composer-bin-plugin": "^1.4.1",
        "guzzlehttp/psr7": "^1.5.2",
        "php-http/guzzle6-adapter": "^2.0.1",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MoabTech\\Procore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoabTech\\Procore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
